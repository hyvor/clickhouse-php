{
    "name": "hyvor/clickhouse-php",
    "description": "ClickhouseDB Client for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyvor\\Clickhouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hyvor\\Clickhouse\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Supun",
            "email": "supun@hyvor.com"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^12.0",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require": {
        "php": ">=8.1",
        "psr/http-client": "^1.0",
        "php-http/discovery": "^1.20",
        "php-http/multipart-stream-builder": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
